<script type="text/javascript">




    function FlushMarket() {
        //不在交易时间段不刷新
        var h = new Date().getHours();
        if(h<9)
        {
            return false;
        }

        $.ajax({
            type: "post",
            url: "./data/FlushMarket.json",
            datatype: "json",
            beforeSend: function () {

            },
            complete: function () {

            },
            success: function (result) {
                var KLineUrl = "/Market/Detail" + "/";

                $("#MarketList tr").each(function (i) {
                    //没有开盘的不刷新
                    if ($(this).attr("isopen") == "true") {
                        var item = result.data[i];

                        //升值币精度4
                        var intFormatFloat = 4;
                        if (item.ProductCode == '900001') {
                            intFormatFloat = f4;
                        }
                        if (item.ProductCode == '800001') {
                            intFormatFloat = 5;
                        }
                        if (item.ProductCode == '800008' || item.ProductCode == '800013') {
                            intFormatFloat = 2;
                        }

                        $(this).find("td").each(function (j) {

                            $(this).removeClass('color-1');
                            $(this).removeClass('color-4');

                            switch (j) {

                                case 2:
                                    
                                    var lastprice = $(this).html();
                                    
                                    if (lastprice < item.Price) {
                                        $(this).addClass('color-1');
                                    }
                                    else if (lastprice > item.Price) {
                                        $(this).addClass('color-4');
                                    }
                                    $(this).html("<em>" + item.Price.toFixed(intFormatFloat) + "</em>")
                                    if (item.Price > item.OpenPrice) {
                                        $(this).attr("class", "color-1");
                                    }
                                    else if (item.Price < item.OpenPrice) {
                                        $(this).attr("class", "color-4");
                                    }
                                    break;
                                case 3:
                                    $(this).html("<em>" + (item.IncreaseRate * 100).toFixed(2) + "%" + "</em>")

                                    if (item.Price > item.OpenPrice) {
                                        $(this).attr("class", "color-1");
                                    }
                                    else if (item.Price < item.OpenPrice) {
                                        $(this).attr("class", "color-4");
                                    }
                                    break;
                                case 4:
                                    $(this).html("<em>" + item.Increase.toFixed(intFormatFloat) + "</em>")

                                    if (item.Price > item.OpenPrice) {
                                        $(this).attr("class", "color-1");
                                    }
                                    else if (item.Price < item.OpenPrice) {
                                        $(this).attr("class", "color-4");
                                    }
                                    break;
                                case 5:
                                    $(this).html(item.OpenPrice.toFixed(intFormatFloat))
                                    break;
                                case 6:
                                    $(this).html("<em>" + item.HighestPrice.toFixed(intFormatFloat) + "</em>")

                                    if (item.HighestPrice > item.OpenPrice) {
                                        $(this).attr("class", "color-1");
                                    }
                                    else if (item.HighestPrice < item.OpenPrice) {
                                        $(this).attr("class", "color-4");
                                    }
                                    break;
                                case 7:
                                    $(this).html("<em>" + item.LowestPrice.toFixed(intFormatFloat) + "</em>")

                                    if (item.LowestPrice > item.OpenPrice) {
                                        $(this).attr("class", "color-1");
                                    }
                                    else if (item.LowestPrice < item.OpenPrice) {
                                        $(this).attr("class", "color-4");
                                    }
                                    break;
                                case 8:
                                    $(this).html("<em>" + item.Volume + "</em>")
                                    break;
                                default:
                                    break;
                            }
                        });

                    }

                });

                $("#MarketList em").addClass("flipy");

            }
        });
    }

    $(function () {
        setInterval('FlushMarket()', 5000);
    });
</script>
