</form><script type="text/javascript">
    function FormatIntValue(value) {
        value = value.replace(/[^\d]/g, "");
        return value;
    }

    $("#Amount").keyup(function () {
        $(this).val(FormatIntValue($(this).val()));

        if ($(this).val() - $("#Balance").html() > 0) {
            $(this).css('color', '#FF0000');
            $(this).val(parseInt($("#Balance").html()));
        } else {
            $(this).css('color', '#888');
        }
    });

    function PreSubmit() {
        if ($("#Amount").val() - 100 < 0) {
            alert("提现的最低限额为100金币");
            return false;
        }
        if ($("#Pwd").val() == '') {
            alert("请输入支付密码");
            return false;
        }
        $("#PayPwd").val(md5($("#Pwd").val() + $("#Pwd").val()));
        $("form").submit();
    }

    function Cancel(OrderId) {
        if (confirm('您确定撤消本次提现申请?')) {
            $.ajax({
                type: "post",
                data: { OrderId: OrderId },
                url: "/User/CancelWithdraw",
                datatype: "json",
                beforeSend: function () {
                },
                complete: function () {
                },
                success: function (result) {
                    if (result.success) {
                        alert(result.message);
                        window.location.href = '/User/Withdraw';
                    } else {
                        alert(result.message);
                    }
                },
                error: function (XMLHttpRequest, textStatus, errorThrown) {
                    alert("操作失败，请稍候再试！")
                }
            });
        }
    }
</script>
